  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server:dev": "cd server && npm run dev",
    "server:install": "cd server && npm install",
    "setup": "npm install && npm run server:install",
    "dev:full": "concurrently \"npm run dev\" \"npm run server:dev\"",
    "dev:both": "concurrently \"npm run dev\" \"cd server && npm run dev\"",
    "kill-ports": "npx kill-port 3001 3002 3003 8080",
    "post-build-ping": "node scripts/ping-search-engines.js",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
